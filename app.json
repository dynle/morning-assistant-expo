{
  "expo": {
    "entryPoint": "./src/App.tsx",
    "name": "MorningAssistant_Expo",
    "slug": "MorningAssistant_Expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.reactjs.native.MorningAssistant",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.1006268244497-b6pqec5i5s09ee71fjulkt2bajes8q2v"
        }
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.morningassistant",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
